<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<DialogPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.app.labtronic.ui.NewCalibrationDialogController" prefHeight="400.0" prefWidth="600.0">
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Edit profile name: " GridPane.rowIndex="0" GridPane.columnIndex="0"
                   GridPane.halignment="RIGHT"/>
            <TextField fx:id="profileNameTextField" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Balance: " GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.halignment="RIGHT"/>
            <Label fx:id="balanceLabel" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Highest win: " GridPane.rowIndex="2" GridPane.columnIndex="0" GridPane.halignment="RIGHT"/>
            <Label fx:id="highestWinLabel" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
